<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jdkgroup.drawerbehavior">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.DefaultDrawerActivity">
        </activity>
        <activity
            android:name=".activity.AdvanceDrawer1Activity">
        </activity>
        <activity
            android:name=".activity.AdvanceDrawer2Activity">
        </activity>
        <activity
            android:name=".activity.AdvanceDrawer3Activity">
        </activity>
        <activity
            android:name=".activity.AdvanceDrawer4Activity">
        </activity>
        <activity
            android:name=".activity.AdvanceDrawer5Activity">
        </activity>
        <activity
            android:name=".activity.Advance3DDrawer1Activity">
        </activity>
    </application>

</manifest>